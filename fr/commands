>(*)==if [[ "$GOTOSORTIPUCETIQUE" == "Fin" ]]; then GOTOSORTIPUCETIQUE=""; commands="$(jv_get_commands)"; jv_handle_order "GOTOSORTI_MOMO"; return; else REPONSEMOMO="$order"; commands="$(jv_get_commands)"; jv_handle_order "prochaine puce "; return; fi
